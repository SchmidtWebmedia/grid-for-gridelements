{
    "name": "schmidtwebmedia/grid-for-gridelements",
    "type": "typo3-cms-extension",
    "description": "F\u00fcr im Backend die M\u00f6glichkeit eines Grids f\u00fcr GridElements hinzu. z.B. f\u00fcr Bootstrap 4",
    "authors": [
        {
            "name": "Marco Schmidt",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "SchmidtWebmedia\\GridForGridelements\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SchmidtWebmedia\\GridForGridelements\\Tests\\": "Tests"
        }
    },
    "replace": {
        "grid_for_gridelements": "self.version",
        "typo3-ter/grid-for-gridelements": "self.version"
    }
}