{
    "name": "schmidtwebmedia/grid-for-gridelements",
    "type": "typo3-cms-extension",
    "description": "Add an easy to use grid element with GridElements. It's Framework independent. Prepared for Bootstrap v4.",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3",
        "extension",
        "bootstrap",
        "gridelements",
        "grid"
      ],
    "authors": [
        {
            "name": "Marco Schmidt",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^10.4",
        "gridelementsteam/gridelements": "^10.0.0"
    },
    "autoload": {
        "psr-4": {
            "SchmidtWebmedia\\GridForGridElements\\": "Classes/"
        }
    },
    "replace": {
        "typo3-ter/grid-for-gridelements": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "grid_for_gridelements"
        }
    }
}
